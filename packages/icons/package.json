{
  "name": "pcln-icons",
  "version": "3.0.0-3",
  "description": "Priceline React icons based on Material Design Icons",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "prepare": "npm run clean && npm run build:svg && npm run build:copy && npm run build:index && npx rollup -c -i components/index.js",
    "clean": "rm -rf node_modules components lib",
    "build:svg": "svgr svg -d components",
    "build:index": "node ./create-index.js",
    "build:copy": "cp src/Icon.js src/Svg.js src/index.js components",
    "build:babel": "babel components -d lib",
    "test": "jest"
  },
  "author": "Priceline",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "styled-system": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@svgr/cli": "^4.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.10.2"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0",
    "styled-components": ">=2.0.0 || >=3.0.0 || >=4.0.0"
  },
  "jest": {
    "setupFilesAfterEnv": ["../../test-setup.js"],
    "testMatch": ["<rootDir>/test/**/*.js"]
  },
  "engines": {
    "node": ">=8.0.0 <9.0.0 || >=10.0.0",
    "npm": ">=5.10.0 || >=6.0.0"
  },
  "gitHead": "f71af5eb40157567f78172a2b58471d7b28dda2e"
}
